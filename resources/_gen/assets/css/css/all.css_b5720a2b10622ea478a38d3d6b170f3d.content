ntent h4:before{content:counter(h2Counter)"." counter(h3Counter)"." counter(h4Counter)". "}.article-content h5:before{content:counter(h2Counter)"." counter(h3Counter)"." counter(h4Counter)"." counter(h5Counter)". "}.article-content h6:before{content:counter(h2Counter)"." counter(h3Counter)"." counter(h4Counter)"." counter(h5Counter)"." counter(h6Counter)". "}table{border-collapse:collapse;border:1px solid var(--gray-deep);width:100%;overflow:auto;font-size:.875em}table:not(.highlight *){white-space:nowrap;margin-top:10px}table thead{border-bottom:3px solid var(--gray-deep)}table thead th{color:var(--black)}table td,table th{padding:.5em 1em;border:1px solid var(--gray-deep)}tbody:not(.highlight tbody) tr:nth-of-type(odd){background-color:var(--gray-light)}tbody:not(.highlight tbody) tr:hover{transition:all .4s;background-color:var(--gray)}kbd{display:inline-block;padding:.25em;background-color:var(--white);border:1px solid var(--gray);border-bottom-color:var(--gray-deep);border-radius:3px;box-shadow:inset 0 -1px 0 var(--gray-deep);font-size:.8em;line-height:1.25;font-family:sfmono-regular,liberation mono,roboto mono,Menlo,Monaco,Consolas,courier new,Courier,monospace;color:var(--black)}.article-matedata a,.article-content a:not(h1 a,h2 a,h3 a,h4 a,h5 a,h6 a){color:var(--orange);transition:all .3s}.article-matedata a:hover,.article-content a:not(h1 a,h2 a,h3 a,h4 a,h5 a,h6 a):hover{color:var(--orange-deep)}.article-content h1 a,.article-content h2 a,.article-content h3 a,.article-content h4 a,.article-content h5 a,.article-content h6 a{color:var(--black);text-decoration:none}.article-content h1 a:hover,.article-content h2 a:hover,.article-content h3 a:hover,.article-content h4 a:hover,.article-content h5 a:hover,.article-content h6 a:hover{text-decoration:underline}.page-nav{width:100%;display:grid;grid-auto-flow:column;justify-content:space-between;grid-template-columns:1fr 1fr;gap:30px;padding-top:10px;border-top:1px solid #eee;margin-top:40px;font-size:.875em;line